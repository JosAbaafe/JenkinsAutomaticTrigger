pipeline{
    agent any
    stages{
        stage('Git Checkout'){
            steps{
                echo "Checking Out Git";
            }
        }
        stage('Build'){
            steps{
                echo "Run Build Successful";
                echo "Hello"
            }
        }
        stage('Unit-Test'){
            steps{
                echo "Running Unit Test";
            }
        }
    }
}